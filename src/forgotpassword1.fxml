<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="anchor_pane" prefHeight="600.0" prefWidth="600.0" stylesheets="LogIn.css" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane fx:id="exit_cover" layoutY="-1.0" prefHeight="31.0" prefWidth="600.0" />
        <HBox fx:id="h_box" alignment="CENTER" layoutY="28.0" prefHeight="100.0" prefWidth="600.0" spacing="5.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="63.0" prefWidth="177.0" text="Pharmacy" textAlignment="CENTER">
                    <font>
                        <Font name="Berlin Sans FB" size="40.0" />
                    </font>
                </Label>
                <ImageView fitHeight="100.0" fitWidth="94.0" layoutX="1000.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@pharmacy.png" />
                    </image></ImageView>
            </children>
        </HBox>
        <Polygon fx:id="polygon" fill="DODGERBLUE" layoutX="49.5" layoutY="90.0" rotate="90.0" stroke="BLACK" strokeType="INSIDE">
            <points>
                <Double fx:value="-50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="50.0" />
                <Double fx:value="40.0" />
                <Double fx:value="0.0" />
                <Double fx:value="-60.0" />
            </points>
        </Polygon>
        <HBox layoutX="38.0" layoutY="150.0" prefHeight="63.0" prefWidth="518.0">
            <children>
                <Label fx:id="lable_log_in" prefHeight="56.0" prefWidth="368.0" text="Forgot password">
                    <font>
                        <Font name="Bell MT Bold" size="48.0" />
                    </font>
                </Label>
            </children>
        </HBox>
      <VBox fx:id="boarder" alignment="TOP_CENTER" layoutX="140.0" layoutY="240.0" prefHeight="285.0" prefWidth="326.0">
         <children>
            <ImageView fx:id="email_icon" fitHeight="78.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@icons8-email-64.png" />
               </image>
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </ImageView>
            <Label fx:id="registered" alignment="CENTER" prefHeight="31.0" prefWidth="268.0" text="Please enter your registered email ID">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin></Label>
            <TextField prefHeight="26.0" prefWidth="504.0" promptText="Please enter your registered email ID">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin></TextField>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <TilePane alignment="TOP_CENTER" layoutX="2.0" layoutY="467.0" prefHeight="83.0" prefWidth="600.0">
         <children>
            <Button fx:id="cancel" mnemonicParsing="false" prefWidth="70.0" text="Cancel">
               <TilePane.margin>
                  <Insets right="10.0" />
               </TilePane.margin>
            </Button>
            <Button fx:id="next" mnemonicParsing="false" prefWidth="70.0" text="Next">
               <TilePane.margin>
                  <Insets left="10.0" />
               </TilePane.margin>
            </Button>
         </children>
      </TilePane>
    </children>
</AnchorPane>
